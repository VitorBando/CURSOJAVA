package fundamentos;

public class DesafioAritimeticos {
	
	public static void main(String[] args) {
		
		int a = (3 + 2);
		int b = (int) Math.pow(a, 2);
		int c = b * 6;
		
		System.out.println(c);
		
		System.out.println((2 + 3) * 6);
		System.out.println(3*2);
		System.out.println(30-6);
				
	}

}
