package fundamentos;

public class DesafioAritimeticos {
	
	public static void main(String[] args) {
		
		int a = 1 - 5;
		int b = 2 - 7
		int c = a * b;
		int d = c / 2;
		int e = (int) Math.pow(d, 2);
		
		System.out.println(e);
		
		System.out.println((2 + 3) * 6);
		System.out.println(3*2);
		System.out.println(30-6);
				
	}

}
