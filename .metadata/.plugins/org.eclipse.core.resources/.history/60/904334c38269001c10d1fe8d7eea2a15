package fundamentos;

public class DesafioAritimeticos {
	
	public static void main(String[] args) {
		
		int a1 = 1 - 5;
		int a2 = 2 - 7;
		int a3 = a1 * a2;
		int a4 = a3 / 2;
		int a5 = (int) Math.pow(a4, 2);
		System.out.println(a5);
	
		int b1 = 3 + 2;
		int b2 = b1 * 6;
		int b3 = 3 * 2;
		int b4 = b2 / b3;
		int b5 = (int) Math.pow(b2 , 2);
		System.out.println(b5);
		
		
	}

}
