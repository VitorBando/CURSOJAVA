package fundamentos;

public class DesafioAritimeticos {
	
	public static void main(String[] args) {
		
		int a = 6 * (2 + 3);
		int b = a * 3 * 2;
		int c = (int) Math.pow(b, 2);
		
		System.out.println(c);
		
		System.out.println((2 + 3) * 6);
		System.out.println(3*2);
		System.out.println(30-6);
				
	}

}
