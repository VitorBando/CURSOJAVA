package fundamentos;

public class Temperatura {
	
	public static void main(String[] args) {
		// (ºF - 32) * 5/9 = ºC
		
		double ajuste = 86;
		double multi = 5.0/9.0;
		double F = -6;
		
		double calculo = (F - ajuste) * multi;
		
		System.out.println("O resultiado é " + calculo + "ºC.");
		
	}
}
